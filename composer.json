{
    "name": "serendipity_hq/php_value_objects",
    "description": "A set of value objects to manage simple and composite values",
    "type": "library",
    "license": "MIT",
    "keywords": ["value objects"],
    "homepage": "https://github.com/Aerendir/PHPValueObjects",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.1",
        "darsyn/ip": "^4",
        "egulias/email-validator": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.8",
        "moneyphp/money": "^3.0",
        "thecodingmachine/safe": "^1.0",
        "zendframework/zend-uri": "^2.5"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
        "doctrine/orm": "^2.5",
        "drenso/phan-extensions": "^2.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phan/phan": "^2.4",
        "phpstan/phpstan": "^0.12.5",
        "phpstan/phpstan-phpunit": "^0.12.6",
        "phpunit/phpunit": "^6||^7||^8||^9",
        "psalm/plugin-phpunit": "^0.8.1",
        "rector/rector": "^0.6.10",
        "sensiolabs/security-checker": "^4.1||^5||^6",
        "symfony/form": "^4||^5",
        "symfony/var-dumper": "^2.8||^3.0||^4||^5",
        "twig/extensions": "^1.0",
        "twig/twig": "~2.7||^3.0",
        "vimeo/psalm": "^3.8"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Component\\ValueObjects\\": "src",
            "SerendipityHQ\\Component\\ValueObjects\\Tests\\": "tests"
        }
    }
}
