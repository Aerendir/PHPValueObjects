{
    "name": "serendipity_hq/php_value_objects",
    "description": "A set of value objects to manage simple and composite values",
    "type": "library",
    "license": "MIT",
    "keywords": ["value objects"],
    "homepage": "https://github.com/Aerendir/PHPValueObjects",
    "authors": [
        {
            "name": "Adamo Aerendir Crespi",
            "email": "hello@aerendir.me"
        }
    ],
    "require": {
        "php": "^7.2",
        "darsyn/ip": "^4",
        "egulias/email-validator": "^2.1",
        "giggsey/libphonenumber-for-php": "^8.8",
        "laminas/laminas-uri": "^2.6.2",
        "moneyphp/money": "^3.0",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.3",
        "dg/bypass-finals": "^1.1",
        "doctrine/dbal": "^2.5",
        "doctrine/orm": "^2.5",
        "drenso/phan-extensions": "2.5.0",
        "phan/phan": "^2.4.6",
        "phpmetrics/phpmetrics": "^2.5",
        "psalm/plugin-phpunit": "0.10.0",
        "rector/rector": "^0.6.11 || ^0.7.0",
        "roave/security-advisories": "dev-master",
        "symfony/form": "^3||^4||^5",
        "symfony/var-dumper": "^2.8||^3.0||^4||^5",
        "twig/intl-extra": "^2.12|^3.0",
        "vimeo/psalm": "3.9.3"
    },
    "suggest": {
        "ext-intl": "Allows to use formatters for various values objects",
        "doctrine/orm": "Allows to use the builtin Doctrine types",
        "symfony/form": "Allows to use the builtin form types for Symfony Form",
        "twig/intl-extra": "Allows to use filters for Twig to localize Money objects"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "SerendipityHQ\\Component\\ValueObjects\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SerendipityHQ\\Component\\ValueObjects\\Tests\\": "tests"
        }
    }
}
